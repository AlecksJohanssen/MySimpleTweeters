<resources>

    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.DarkActionBar">'
        <item name="colorControlNormal">#4099FF</item>
        <item name="colorControlActivated">#4099FF</item>
        <item name="colorControlHighlight">#4099FF</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="colorControlNormal">#4099FF</item>
        <item name="colorControlActivated">#4099FF</item>
        <item name="colorControlHighlight">#4099FF</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar"></style>
    <style name="ActionBarTabText.AppBarOverlay" parent="@android:style/Widget.Holo.Light.ActionBar.TabText">
        <item name="android:textColor">#4099FF</item>
    </style>
    <style name="MyMaterialTheme.TabLayout" parent="Widget.Design.TabLayout">
        <item name="tabIndicatorColor">#4099FF</item>
        <item name="tabIndicatorHeight">3dp</item>
        <item name="tabTextAppearance">@style/MyMaterialTheme.TabLayout.TextAppearance</item>
        <item name="tabSelectedTextColor">#4099FF</item>
        <item name="android:background">#ffff</item>
    </style>

    <style name="MyMaterialTheme.TabLayout.TextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#4099FF</item>
        <item name="textAllCaps">true</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="counterText">
        <item name="android:textColor">#4099FF</item>
    </style>
    <style name="counterOverride">
        <item name="android:textColor">#ff0000</item>
    </style>
    <style name="Theme.App.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlNormal">#000</item>
        <item name="colorControlActivated">#4099FF</item>
        <item name="colorControlHighlight">#000</item>
    </style>
    <style name="testAtrribute">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:color">#4099FF</item>

</style>

</resources>
